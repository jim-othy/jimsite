{
  "hosting": {
    "public": "public",
    "cleanUrls": true,
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/archive",
        "destination": "/Pages/Archive/archive.html"
      },
      {
        "source": "/exam.ple",
        "destination": "/Pages/Projects/Projects/exam.ple.html"
      }
    ]
  }
}
