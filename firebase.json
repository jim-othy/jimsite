{
  "hosting": {
    "public": "public",
    "cleanUrls": true,
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/archive",
        "destination": "/Pages/archive.html"
      },
      {
        "source": "/feed",
        "destination": "/Pages/feed.xml"
      },
      {
        "source": "/about",
        "destination": "/Pages/about.html"
      },
      {
        "source": "/archives/4041",
        "destination": "/Pages/Archives/4041.html"
      },
      {
        "source": "/archives/launch",
        "destination": "/Pages/Archives/launch.html"
      },
      {
        "source": "/archives/webv1",
        "destination": "/Pages/Archives/webv1.html"
      },
      {
        "source": "/archives/webv2",
        "destination": "/Pages/Archives/webv2.html"
      }
    ]
  }
}
